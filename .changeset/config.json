{
  "$schema": "https://unpkg.com/@changesets/config@2.1.0/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    { "repo": "n1ru4l/graphql-live-query" }
  ],
  "commit": false,
  "linked": [
    ["@n1ru4l/socket-io-graphql-server", "@n1ru4l/socket-io-graphql-client"]
  ],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "ignore": [
    "@n1ru4l/todo-example-server",
    "@n1ru4l/todo-example-server-helix",
    "@n1ru4l/todo-example-server-ws",
    "@n1ru4l/todo-example-client-apollo",
    "@n1ru4l/todo-example-client-relay",
    "@n1ru4l/todo-example-client-urql",
    "@n1ru4l/end2end-tests"
  ],
  "snapshot": {
    "useCalculatedVersion": true,
    "prereleaseTemplate": "{tag}-{datetime}-{commit}"
  }
}
